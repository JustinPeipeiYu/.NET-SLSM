<Window x:Class="Smoke_Less_2024.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:gif="http://wpfanimatedgif.codeplex.com"
        xmlns:local="clr-namespace:Smoke_Less_2024"
        Icon="pack://siteoforigin:,,,/cute heart beating.ico"
        mc:Ignorable="d"
        Title="MainWindow" ResizeMode="NoResize" Height="480" Width="800"
        Background="black">
    <Grid Loaded="MainWindow_Loaded">
        <Image x:Name="Gif" Source="pack://siteoforigin:,,,/cute heart beating.gif" Margin="0,0,0,0" Opacity="0.5"/>
        <Slider x:Name="Count_sld" HorizontalAlignment="Center" Margin="0,425,0,0" VerticalAlignment="Top" Width="592" Height="29" Maximum="25" SmallChange="1" TickPlacement="TopLeft" AutoToolTipPlacement="TopLeft" FontFamily="Impact" Cursor="Hand"/>
        <Canvas Margin="72,62,52,65" Background="transparent"/>
    </Grid>
</Window>
